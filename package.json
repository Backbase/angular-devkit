  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Backbase/angular-devkit/issues"
  },
  "homepage": "https://github.com/Backbase/angular-devkit#readme",
  "devDependencies": {
    "@angular-devkit/architect": "^0.803.28",
    "@types/jest": "^26.0.3",
    "@types/jsdom": "^16.2.3",
    "gulp": "^4.0.2",
    "gulp-typescript": "^5.0.1",
    "jest": "^26.1.0",
    "merge2": "^1.4.1",
    "prettier": "2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "jsdom": "^16.2.2",
    "ncp": "^2.0.0",
