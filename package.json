{
  "name": "@bb-cli/angular-devkit",
  "version": "0.2.0",
  "description": "Angular dev tools for Backbase projects",
  "scripts": {
    "generate:sources": "json2ts -i src/builders/cx-package/schema.json -o src/builders/cx-package/schema.d.ts",
    "prebuild": "rm -rf dist && npm run generate:sources",
    "build": "tsc -p tsconfig.lib.json",
    "postbuild": "cp README.md LICENSE dist",
    "test": "jest",
    "lint": "eslint src",
    "format": "npx prettier --write src"
  },
  "bin": {
    "xliff-join": "./node_modules/xlf-merge/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Backbase/angular-devkit.git"
  },
  "author": "Backbase",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Backbase/angular-devkit/issues"
  },
  "homepage": "https://github.com/Backbase/angular-devkit#readme",
  "devDependencies": {
    "@angular-devkit/architect": "^0.803.28",
    "@types/jest": "^26.0.3",
    "@types/node": "^14.0.26",
    "@babel/types": "7.8.3",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "glob": "^7.1.6",
    "jest": "^26.1.0",
    "json-schema-to-typescript": "^9.1.1",
    "memfs": "^3.2.0",
    "prettier": "2.0.5",
    "ts-jest": "^26.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5",
    "unzipper": "^0.10.11"
  },
  "dependencies": {
    "@angular/compiler": "10.0.8",
    "@angular/compiler-cli": "^10.0.8",
    "@angular-devkit/schematics": "10.0.5",
    "@angular/localize": "^10.0.8",
    "@babel/core": "7.8.3",
    "fs-extra": "^9.0.1",
    "xlf-merge": "^1.0.6",
    "ncp": "^2.0.0",
    "parse5": "^6.0.0",
    "rimraf": "^3.0.2",
    "xmldom": "^0.3.0",
    "zip-folder": "^1.0.0"
  },
  "builders": "./src/builders/builders.json"
}
