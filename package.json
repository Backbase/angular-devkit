{
  "name": "@backbase/angular-devkit",
  "version": "0.1.0",
  "description": "Angular dev tools for Backbase projects",
  "scripts": {
    "generate:sources": "npx -p json-schema-to-typescript json2ts -i src/builders/cx-package/schema.json -o src/builders/cx-package/schema.d.ts",
    "prebuild": "rm -rf ./dist && npm run generate:sources",
    "build": "gulp",
    "test": "jest",
    "format": "npx prettier --write src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Backbase/angular-devkit.git"
  },
